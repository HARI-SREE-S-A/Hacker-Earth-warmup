T = int(input())
iN = []
for i in range(T):
    iN = [int(x) for x in input().split()]
    N = ((iN[0])/iN[2])
    if iN[0]%iN[2] != 0:
        N += 0.5
  
  
    M = ((iN[1])/iN[2])
    if iN[1]%iN[2] != 0:
        M += 0.5
    x = round((M + N))
    if x <= 1:
        x = x+1
        
    print(x)
#####################################################################################
 = int(input())
for i in range(t):
    n,m,k = map(int, input().split()) 
    if (n % k) != 0:
        n += k - (n%k)
    if (m % k) != 0:
        m += k - (m%k)
    print((n//k) + (m//k))
